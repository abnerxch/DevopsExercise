<?xml version="1.0" encoding="iso-8859-1"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Example" preserve-order="true" allow-return-values="true" verbose="1">
    <test name="Example">
        <groups>
            <run>
                <exclude name="brokenTests" />
                <include name="Testing Edad" />
                <include name="Testing ID"/>
                <include name="Testing Nacimiento"/>
                <include name="Testing Nombre"/>
                <include name="Testing Nota"/>
                <include name="Testing Sexo"/>
            </run>
        </groups>
        <classes>
            <class name="TestEdad"/>
            <class name="TestID"/>
            <class name="TestNacimiento"/>
            <class name="TestNombre"/>
            <class name="TestNota"/>
            <class name="TestSexo"/>
        </classes>
    </test>

    <listeners>
        <listener class-name="ListenerSuite"/>
        <listener class-name="ListenerTest"/>
    </listeners>
</suite> 